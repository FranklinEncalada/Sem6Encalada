<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Sem6Encalada.MainPage">

    <StackLayout>
        <Frame BackgroundColor="#2196F3" Padding="24" CornerRadius="0">
            <Label Text="Universidad Israel" HorizontalTextAlignment="Center" TextColor="White" FontSize="36"/>
        </Frame>
        <Label Text="Seleccione la opcion " FontSize="Title" Padding="30,10,30,10"/>
        <Button x:Name="btninsertar" Text="Insertar" Clicked="btninsertar_Clicked"/>
        <Button x:Name="btneliminar" Text="Eliminar" Clicked="btneliminar_Clicked"/>
        <Button x:Name="btnmodificar" Text="Modificar" Clicked="btnmodificar_Clicked"/>
    </StackLayout>

</ContentPage>
